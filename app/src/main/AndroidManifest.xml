<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taxijjangnews">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_taxi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_taxi_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TaxiJJangNews"
        android:usesCleartextTraffic="true">
        <activity android:name=".newspage.NewsActivity"></activity>
        <activity android:name=".platformpage.PlatformPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".newslistpage.NewsListPageActivity" />
    </application>

</manifest>